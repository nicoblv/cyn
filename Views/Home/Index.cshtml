@{
	Layout = null;
}
<!DOCTYPE html>
<html>
<head>
	<title>Catalina & Nicolás</title>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">

	<!--Custom styles-->
	<link rel="stylesheet" type="text/css" href="~/css/site.css">
</head>
<body>
	<div class="container">
		<div class="d-flex justify-content-center h-100">
			<div class="card">
				<div class="card-header">
					<h3>Ingrese código de acceso</h3>
				</div>
				<div class="card-body">
					<form action="@ViewBag.ReturnUrl" method="POST">
						<div class="input-group form-group">
							<div class="input-group-prepend">
								<span class="input-group-text"><i class="fas fa-user"></i></span>
							</div>
                            <input type="text" id="inputCode" name="inputCode" class="form-control" placeholder="Código acceso" required>

						</div>
						<div class="form-group">
                            <input type="submit" value="Ingresar" class="btn float-center login_btn">
						</div>
						<div id="msjError"></div>
					</form>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
